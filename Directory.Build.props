<Project>
    <PropertyGroup>
        <!-- Do not add <AssemblyVersion> here. Each assembly should receive a unique assembly version. -->
        <Company>SwifterTheDragon</Company>
        <Copyright>Copyright SwifterTheDragon, and the SwifterTheDragon.VerboseSourceRevisionIdBuilder contributors, 2024-2025. All Rights Reserved.</Copyright>
        <!-- Do not add <Description> here. Each assembly should receive a unique description. -->
        <!-- Do not add <FileVersion> here. Each assembly should receive a unique file version. -->
        <IncludeSourceRevisionInInformationalVersion>false</IncludeSourceRevisionInInformationalVersion>
        <InformationalVersion>3.0.0</InformationalVersion>
        <Product>SwifterTheDragon.VerboseSourceRevisionIdBuilder</Product>
        <Title>$(AssemblyName)</Title>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <Authors>SwifterTheDragon</Authors>
        <PackageProjectUrl>https://github.com/SwifterTheDragon/VerboseSourceRevisionIdBuilder</PackageProjectUrl>
        <RepositoryUrl>https://github.com/SwifterTheDragon/VerboseSourceRevisionIdBuilder</RepositoryUrl>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    </PropertyGroup>
    <ItemGroup>
        <AssemblyAttribute Include="System.CLSCompliantAttribute">
            <_Parameter1>true</_Parameter1>
        </AssemblyAttribute>
        <AssemblyAttribute Include="System.Reflection.AssemblyDefaultAliasAttribute">
            <_Parameter1>$(AssemblyName)</_Parameter1>
        </AssemblyAttribute>
        <AssemblyAttribute Include="System.Runtime.InteropServices.ComVisibleAttribute">
            <_Parameter1>true</_Parameter1>
        </AssemblyAttribute>
        <AssemblyAttribute Include="System.Resources.NeutralResourcesLanguageAttribute">
            <_Parameter1>en-001</_Parameter1>
        </AssemblyAttribute>
    </ItemGroup>
    <PropertyGroup>
        <RepositoryRoot>$(MSBuildThisFileDirectory)</RepositoryRoot>
    </PropertyGroup>
    <ItemGroup>
        <AdditionalFiles Include="$(RepositoryRoot)SonarLint.xml" />
        <AdditionalFiles Include="$(RepositoryRoot)CodeMetricsConfig.txt" />
    </ItemGroup>
</Project>
